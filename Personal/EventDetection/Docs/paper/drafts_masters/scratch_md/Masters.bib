@article{struzik_wavelet_2002,
	title = {Wavelet transform based multifractal formalism in outlier detection and localisation for financial time series},
	volume = {309},
	issn = {0378-4371},
	url = {http://www.sciencedirect.com/science/article/pii/S0378437102005526},
	doi = {10.1016/S0378-4371(02)00552-6},
	abstract = {We present a method of detecting and localising outliers in financial time series and other stochastic processes. The method checks the internal consistency of the scaling behaviour of the process within the paradigm of the multifractal spectrum. Deviation from the expected spectrum is interpreted as the potential presence of outliers. The detection part of the method is then supplemented by the localisation analysis part, using the local scaling properties of the time series. Localised outliers can then be removed one by one, with the possibility of dynamic verification of spectral properties. Both the multifractal spectrum formalism and the local scaling properties of the time series are implemented on the wavelet transform modulus maxima tree.},
	number = {3{\textendash}4},
	urldate = {2017-03-30},
	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Struzik, Zbigniew R. and Siebes, Arno P. J. M.},
	month = jun,
	year = {2002},
	keywords = {H{\"o}lder exponent, Multifractal analysis, Outlier detection, Wavelet transform},
	pages = {388--402},
	file = {ScienceDirect Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/JHFKIA8X/Struzik and Siebes - 2002 - Wavelet transform based multifractal formalism in .pdf:application/pdf;ScienceDirect Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/VD5ZT79E/S0378437102005526.html:text/html}
}

@article{grane_wavelet-based_2010,
	series = {The {Fifth} {Special} {Issue} on {Computational} {Econometrics}},
	title = {Wavelet-based detection of outliers in financial time series},
	volume = {54},
	issn = {0167-9473},
	url = {http://www.sciencedirect.com/science/article/pii/S0167947309004629},
	doi = {10.1016/j.csda.2009.12.010},
	abstract = {Outliers in financial data can lead to model parameter estimation biases, invalid inferences and poor volatility forecasts. Therefore, their detection and correction should be taken seriously when modeling financial data. The present paper focuses on these issues and proposes a general detection and correction method based on wavelets that can be applied to a large class of volatility models. The effectiveness of the new proposal is tested by an intensive Monte Carlo study for six well-known volatility models and compared to alternative proposals in the literature, before it is applied to three daily stock market indices. The Monte Carlo experiments show that the new method is both very effective in detecting isolated outliers and outlier patches and much more reliable than other alternatives, since it detects a significantly smaller number of false outliers. Correcting the data of outliers reduces the skewness and the excess kurtosis of the return series distributions and allows for more accurate return prediction intervals compared to those obtained when the existence of outliers is ignored.},
	number = {11},
	urldate = {2017-03-30},
	journal = {Computational Statistics \& Data Analysis},
	author = {Gran{\'e}, Aurea and Veiga, Helena},
	month = nov,
	year = {2010},
