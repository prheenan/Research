	month = jul,
	year = {2011},
	keywords = {atomic force microscopy, Characterization, Fuzzy logic, Nanostructures},
	pages = {706--715},
	file = {ScienceDirect Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/8Q3EF97M/Al-Mousa et al. - 2011 - Analysis of atomic force microscopy data for surfa.pdf:application/pdf;ScienceDirect Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/E6REZ68I/S1044580311000775.html:text/html}
}

@article{partola_frame_2016,
