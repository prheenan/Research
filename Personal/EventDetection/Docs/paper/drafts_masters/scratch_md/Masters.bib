	url = {//www.sciencedirect.com/science/article/pii/S0304399199001540},
	doi = {10.1016/S0304-3991(99)00154-0},
	abstract = {The force-distance cycle mode of the atomic force microscope (AFM) allows for detection of interaction forces between the AFM-tip and a substrate (probe). This can either be a direct tip{\textendash}sample interaction or an interaction between molecules coupled to the tip and probe, respectively. The interaction forces are typically in the range of a few pN to some hundred pN. In this article we describe algorithms for the analysis of force{\textendash}distance cycles, to quantify interaction forces between tip and probe. Both, the direct tip{\textendash}probe interaction as well as the interaction between specifically bound molecules are analyzed. The molecules bound to tip and probe have to be either long and flexible or have to be bound via a flexible cross linker. The algorithms are exemplified on direct tip{\textendash}probe interactions and on unbinding events of cadherins which are bound via PEG-spacers to the AFM-tip and to the probe.},
	number = {1{\textendash}4},
	urldate = {2017-01-20},
	journal = {Ultramicroscopy},
	author = {Baumgartner, W. and Hinterdorfer, P. and Schindler, H.},
	month = feb,
	year = {2000},
	keywords = {afm, atomic force microscopy, force spectroscopy, Unbinding},
	pages = {85--95},
	file = {ScienceDirect Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/2TF9GA2H/Baumgartner et al. - 2000 - Data analysis of interaction forces measured with .pdf:application/pdf;ScienceDirect Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/GK3QZ7JV/S0304399199001540.html:text/html}
}

@article{klapetek_independent_2014,
	title = {Independent analysis of mechanical data from atomic force microscopy},
	volume = {25},
	issn = {0957-0233},
	url = {http://stacks.iop.org/0957-0233/25/i=4/a=044009},
	doi = {10.1088/0957-0233/25/4/044009},
	abstract = {Present atomic force microscopes are capable of acquiring large data volumes by point using point force{\textendash}distance spectroscopic measurements. Even if different trade names and different technical implementations are used, for most of these techniques a force{\textendash}distance curve in every image pixel is measured, this curve is immediately fitted by some theoretical dependence and results are displayed as a mechanical properties channel (Young modulus, adhesion, etc). Results are processed during the measurement directly in the scanning probe microscopy controller or, after it, by manufacturer provided software. In this paper, we present a software tool for independent numerical processing of such data, including more numerical models for the force{\textendash}distance curve evaluation and including a simple estimate of uncertainties related to the fitting procedure. This can improve the reliability and the analytical possibilities of mechanical properties mapping methods in an atomic force microscopy.},
	language = {en},
	number = {4},
	urldate = {2017-01-20},
