	author = {Baumgartner, W. and Hinterdorfer, P. and Schindler, H.},
	month = feb,
	year = {2000},
	keywords = {afm, atomic force microscopy, force spectroscopy, Unbinding},
	pages = {85--95},
	file = {ScienceDirect Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/WM849P6P/Baumgartner et al. - 2000 - Data analysis of interaction forces measured with .pdf:application/pdf;ScienceDirect Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/KZ7N7H2A/S0304399199001540.html:text/html}
}

@article{casuso_automated_2010,
	title = {Automated setpoint adjustment for biological contact mode atomic force microscopy imaging},
	volume = {21},
	issn = {0957-4484},
	url = {http://stacks.iop.org/0957-4484/21/i=3/a=035104},
	doi = {10.1088/0957-4484/21/3/035104},
	abstract = {Contact mode atomic force microscopy (AFM) is the most frequently used AFM imaging mode in biology. It is about 5{\textendash}10 times faster than oscillating mode imaging (in conventional AFM setups), and provides topographs of biological samples with sub-molecular resolution and at a high signal-to-noise ratio. Unfortunately, contact mode imaging is sensitive to the applied force and intrinsic force drift: inappropriate force applied by the AFM tip damages the soft biological samples. We present a methodology that automatically searches for and maintains high resolution imaging forces. We found that the vertical and lateral vibrations of the probe during scanning are valuable signals for the characterization of the actual applied force by the tip. This allows automated adjustment and correction of the setpoint force during an experiment. A system that permanently performs this methodology steered the AFM towards high resolution imaging forces and imaged purple membrane at molecular resolution and live cells at high signal-to-noise ratio for hours without an operator.},
	language = {en},
	number = {3},
	urldate = {2017-01-20},
	journal = {Nanotechnology},
	author = {Casuso, Ignacio and Scheuring, Simon},
	year = {2010},
	pages = {035104},
	file = {IOP Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/XQSMSQAQ/Casuso and Scheuring - 2010 - Automated setpoint adjustment for biological conta.pdf:application/pdf}
}

@article{xie_characterization_2016,
	title = {Characterization of {Recognition} {Events} between {Proteins} on a {Single} {Molecule} {Level} with {Atomic} {Force} {Microscopy}},
	volume = {55},
	issn = {0888-5885},
	url = {http://dx.doi.org/10.1021/acs.iecr.5b03922},
