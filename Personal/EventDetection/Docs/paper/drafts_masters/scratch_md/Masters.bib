}

@article{garcia-masso_automated_2016,
	title = {Automated detection of protein unfolding events in atomic force microscopy force curves},
	volume = {79},
	issn = {1097-0029},
	url = {http://onlinelibrary.wiley.com.colorado.idm.oclc.org/doi/10.1002/jemt.22764/abstract},
	doi = {10.1002/jemt.22764},
	abstract = {Atomic force microscopy is not only a high-resolution imaging device but also a mechanical machine, which can be used either to indent or stretch (soft) biomaterials. Due to the statistical nature of such materials (i.e., hydrogels or polymers) hundreds of force-distance curves are required to describe their mechanical properties. In this manuscript, we present an automated system for polymer unfolding detection based on continuous wavelet analysis. We have tested the automated program on elastin, which is an important protein that provides elasticity to tissues and organs. Our results show that elastin changes its mechanical behavior in the presence of electrolytes. In particular, we show that NaCl has a different effect on the contour length than CaCl2 for similar unfolding forces. In addition, we provide the program in the supporting information for the researches facing such kind of problem.},
	language = {en},
	number = {11},
	urldate = {2017-01-20},
	journal = {Microscopy Research and Technique},
	author = {Garc{\'i}a-Mass{\'o}, Xavier and Huber, Matthias C. and Friedmann, Jacqueline. and Gonzalez, Luis M. and Schiller, Stefan M. and Toca-Herrera, Jos{\'e} L.},
	month = nov,
	year = {2016},
	keywords = {adhesion, atomic force microscopy, detection program, elastin, unfolding},
	pages = {1105--1111},
	file = {Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/FEU4U3X8/Garc{\'i}a-Mass{\'o} et al. - 2016 - Automated detection of protein unfolding events in.pdf:application/pdf;Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/RE7Z6BMK/abstract\;jsessionid=2D67321089A112F6488AA76D31BBCEAE.html:text/html}
