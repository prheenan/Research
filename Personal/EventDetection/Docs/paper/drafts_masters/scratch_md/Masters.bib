	file = {ACS Full Text PDF w/ Links:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/QDT7I7MT/Askar et al. - 1996 - Wavelet Transform for Analysis of Molecular Dynami.pdf:application/pdf;ACS Full Text Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/JU4UHUW4/jp961961w.html:text/html}
}

@article{beck_methods_2004,
