	url = {https://www.sciencedirect.com/science/article/pii/S0304399113002064},
	doi = {10.1016/j.ultramic.2013.07.020},
	abstract = {The benefits of single molecule force spectroscopy (SMFS) clearly outweigh the challenges which include small sample sizes, tedious data collection and introduction of human bias during the subjective data selection. These difficulties can be partially eliminated through automation of the experimental data collection process for atomic force microscopy (AFM). Automation can be accomplished using an algorithm that triages usable force{\textendash}extension recordings quickly with positive and negative selection. We implemented an algorithm based on the windowed fast Fourier transform of force{\textendash}extension traces that identifies peaks using force{\textendash}extension regimes to correctly identify usable recordings from proteins composed of repeated domains. This algorithm excels as a real-time diagnostic because it involves \&lt;30 ms computational time, has high sensitivity and specificity, and efficiently detects weak unfolding events. We used the statistics provided by the automated procedure to clearly demonstrate the properties of molecular adhesion and how these properties change with differences in the cantilever tip and protein functional groups and protein age.},
	urldate = {2017-02-03},
	journal = {Ultramicroscopy},
	author = {Scholl, Zackary N. and Marszalek, Piotr E.},
	month = jan,
	year = {2014},
	keywords = {Atomic force, Protein adsorption of surfaces, Proteins, Single molecule experiments, Single molecule microscopy, single molecules},
	pages = {7--14},
	file = {ScienceDirect Full Text PDF:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/AAM7WQFF/Scholl and Marszalek - 2014 - Improving single molecule force spectroscopy throu.pdf:application/pdf;ScienceDirect Snapshot:/Users/patrickheenan/Library/Application Support/Zotero/Profiles/ydppxtk0.default/zotero/storage/SDMQF6ET/S0304399113002064.html:text/html}
}

